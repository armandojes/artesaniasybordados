  "main": "/",
  "scripts": {
    "build:localserver": "set NODE_OPTIONS=--openssl-legacy-provider&& webpack --config webpack/local_server.js",
    "build:serverrender": "set NODE_OPTIONS=--openssl-legacy-provider&& webpack --config webpack/server_render",
    "build:api": "set NODE_OPTIONS=--openssl-legacy-provider&& webpack --config webpack/api.js",
    "build:front": "set NODE_OPTIONS=--openssl-legacy-provider&& webpack --config webpack/client.js",
    "watch:front": "set NODE_OPTIONS=--openssl-legacy-provider&& webpack-dev-server --config webpack/client.js",
    "all": "set NODE_ENV=development&& concurrently  \"npm run build:localserver\" \"npm run watch:front\" \"nodemon build/server\"",
    "deploy:production": "set NODE_ENV=production&& npm run build:serverrender&& npm run build:api&& npm run build:front&& vercel --prod",
    "deploy:d": "set NODE_ENV=production&& npm run build:serverrender&& npm run build:api&& npm run build:front&& vercel ",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
